.container.branch-main-content
  .row
    %h1.page-title= page_title
    = render "posts/branch/create_new_post", branch: branch
  .row
    = render 'posts/branch/categories', branch: branch
  .row#feed.container
    .col-sm-12
      = render @posts
      = render no_post_partial_path
  .infinite-scroll
    = will_paginate @posts